@model List<Urun>

<table class="table table-sm table-bordered table-hover mt-2">
    <tr>
        <th>Id</th>
        <th>Resim</th>
        <th>Ad</th>
        <th>Fiyat</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Id</td>
            <td></td>
            <td>@item.Ad</td>
            <td>@item.Fiyat</td>
        </tr>
    }
</table>

